{
  "appenders": {
    "console": {
      "type": "console"
    },
    "file": {
      "type": "dateFile",
      "filename": "./logs/gateway.log",
      "alwaysIncludePattern": false,
      "pattern": ".yyyy-MM-dd-hh"
    }
  },
  "categories": {
    "default": {
      "appenders": ["file", "console"],
      "level": "ALL"
    }
  }
}
